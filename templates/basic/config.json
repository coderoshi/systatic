{
  "sourceDir" : "src",
  "buildDir" : "build",
  "javascripts": {
    "route" : "/javascripts/(.+)",
    "plugin" : "servitude",
    "baseDir" : "javascripts"
  },
  "stylesheets": {
    "route" : "/stylesheets/(.+)",
    "plugin" : "servitude",
    "baseDir" : "stylesheets",
    "ignore": []
  },
  "images": {
    "route" : "/images/(.+)",
    "plugin" : "filehandler",
    "baseDir" : "."
  },
  "ignore": ["layout.jade"],
  "deploy": [{
    "compress" : "true",
    "location" : "S3",
    "access_key_id" : "",
    "secret_access_key" : ""
  }]
}
